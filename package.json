{
  "name": "harp-personal",
  "version": "1.0.0",
  "description": "My personal website written using Harp web server and deployed with Travis and GitHub Pages!",
  "main": "index.js",
  "scripts": {
    "serve": "webpack --config webpack.dev.js; harp server --port $PORT",
    "build": "webpack --config webpack.prod.js; harp compile",
    "webpack": "webpack --config webpack.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pschfr/harp-personal.git"
  },
  "keywords": [
    "harp",
    "harpjs",
    "travis",
    "github-pages",
    "bootstrap",
    "webpack"
  ],
  "author": "Paul Schaefer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pschfr/harp-personal/issues"
  },
  "homepage": "https://github.com/pschfr/harp-personal#readme",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "harp": "^0.29.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "webpack": "^4.29.5",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "webpack-cli": "^3.2.3"
  }
}
